{
  "name": "ang-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node app.js",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "server": "node app.js",
    "client": "ng serve --proxy-config proxy.conf.json",
